<meta charset="UTF-8" /><meta charset="UTF-8" />
<title>Dashboard</title>
<link rel="stylesheet" href="/css/style.css" />
<head>
  <body>
    <main class="container">
      <%- include('../partials/navBar'); %>
      <input class="searchBar" type="search" />
      <div class="overviewContainer">
          <h1 class="overviewHeader">Overview</h1>
        </div>
        <div class = "overviewCard">
          <h3 class="acctBal_head">Account Balance</h3>
          <strong> <%= accountBalance.amount %></strong>
          <a class="viewAll" href="#">View All</a>
          <h4 class="bal"></h4>
        </div>
        <div class = "overviewCard">
          <h3 class="pending_head">Pending</h3>
          <strong> <%= pending.amount %></strong>
          <a class="viewAll" href="#">View All</a>
        </div>
        <div class = "overviewCard">
          <h3 class="processed_head">Processed</h3>
          <strong> <%= processedData.amount %></strong>
          <a class="viewAll" href="#">View All</a>
      </div>
      <div class="activityContainer">
        <div class="recentActivity_header">
          <h1>Recent Activity</h1>
        </div>
      <div class="recentActivity_bar">
        <h3>Project</h3>
        <h3>Start Date</h3>
        <h3>End Date</h3>
        <h3>Team</h3>
        <h3>Status</h3>
      </div>
      </div>
    </main>
  </body>
</head>
